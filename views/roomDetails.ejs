      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link mr-3 ml-2" href="/">HOME</a>
          </li>
          <li class="nav-item">
            <a class="nav-link mr-3 ml-2" href="/admission">ADMISSION</a>
          </li>
          <li class="nav-item">
            <a class="nav-link mr-3 ml-2" href="/ambulance">AMBULANCE</a>
          </li>
          <li class="nav-item">
            <a class="nav-link mr-3 ml-2 active" href="/roomDetails">ROOM DETAILS</a>
          </li>
          <li class="nav-item">
            <a class="nav-link mr-3 ml-2" href="/adminsign">ADMIN</a>
          </li>
          <!-- <li class="nav-item">
            <a class="nav-link mr-3 ml-2" href="/logout">LOGOUT</a>
          </li> -->
        </ul>
      </div>
    </nav>
    <% } else { %>
      <nav class="navbar navbar-expand-lg navbar-dark" style="background-color: rgb(24, 24, 95);">
        <a class="navbar-brand" href="#" style="font-family: 'Spartan', sans-serif;"><i class="fas fa-hospital"></i> KARUR  HEALTH CARE </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2" href="/">HOME</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2" href="/ambulanceDet">AMBULANCE ENTRIES</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2" href="/roomEntry">ROOM ENTRY</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2 active" href="/adminRoom">ROOM DETAILS</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2" href="/admin">ADMISSION DETAILS</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mr-3 ml-2" href="/logout">LOGOUT</a>
            </li>
          </ul>
        </div>
      </nav>
      <% } %>
      <div class="limiter">
        <!-- <a href="/roomEntry"><button class="btn btn-outline-light btn-lg float-lg-right mr-5 mt-4">RETURN TO ROOM ENTRY</button></a> -->
        <div class="pb-5">
        <h2 class="cont1-head admiss-head" style="padding-top: 3%;color: white;">Room Details</h2>
          </div>
          <% if(NoOfEntries > 0) { %>
    <div class="table100 pb-5">
    <table>
    <thead>
    <tr class="table100-head">
    <th class="column1">Sno.</th>
    <th class="column2">Room no.</th>
    <th class="column3">Admission Date</th>
    <th class="column4">Patient Name</th>
    <th class="column5">Patient Age</th>
    <th class="column6">Place</th>
    <th class="column7">Attender Name</th>
    </tr>
    </thead>
    <tbody>
      <% found.forEach((val) => { %>
        <% if(admiss_no == sno ) { %>
          <tr class="data" style="background-color: rgba(113, 113, 153, 0.911);color: white;">
          <td class="column1"><%= sno++ %></td>
          <td class="column2"><%= val.roomNo %></td>
          <td class="column3"><%= val.admissDate %></td>
          <td class="column4"><%= val.name %></td>
          <td class="column5"><%= val.age %></td>
          <td class="column6"><%= val.place %></td>
          <td class="column7"><%= val.attenderName %></td>
        </tr>  
        <% } else { %>
          <tr class="data">
            <td class="column1"><%= sno++ %></td>
            <td class="column2"><%= val.roomNo %></td>
            <td class="column3"><%= val.admissDate %></td>
            <td class="column4"><%= val.name %></td>
            <td class="column5"><%= val.age %></td>
            <td class="column6"><%= val.place %></td>
            <td class="column7"><%= val.attenderName %></td>
          </tr>
          <% } %>
      <% }) %>
    </tbody>
    </table>
    <!-- <div class="text-center"><a href="/roomEntry"><button class="btn btn-success btn-lg mt-4">Make Entry <i class="fa-solid fa-angles-right"></i></button></a></div> -->
    </div>
    <% } else { %>
      <div style="color: white;margin-top: 8%;" class="text-center">
        <div><i class="fa-solid fa-folder-open" style="font-size: 700%;"></i></div>
        <div class="mt-3 fa-2x"><span> NO DETAILS FOUND </span></div>
      </div>  
      <% } %>  
    </div>
    </body>
</html>      